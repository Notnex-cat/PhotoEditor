<Window x:Class="DONT_DELITE_____.Front.Rotate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DONT_DELITE_____.Front"
        mc:Ignorable="d"
        Title="Rotate" Height="200" Width="300">
    <Grid  >
        <TextBox x:Name="angle" HorizontalAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="68,40,0,0" TextWrapping="Wrap" Text="Введите в градусах то насколько вы хотите повернуть изображение" VerticalAlignment="Top"/>
        <Button x:Name="RotateOk" Content="Столько" HorizontalAlignment="Center" Margin="0,130,0,0" VerticalAlignment="Top" Click="RotateOk_Click"/>

    </Grid>
</Window>

<Window x:Class="DONT_DELITE_____.Front.New"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DONT_DELITE_____.Front"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="New" Height="240" Width="400">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="123,45,0,0" TextWrapping="Wrap" Text="Введите ширину и высоту" VerticalAlignment="Top"/>
        <TextBox x:Name="Width" Style="{StaticResource MaterialDesignFloatingHintTextBox}" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Ширина" PreviewTextInput="NumberValidationTextBox" Margin="134,79,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Height" Style="{StaticResource MaterialDesignFloatingHintTextBox}" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Высота" PreviewTextInput="NumberValidationTextBox" Margin="134,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Ok" Content="Ok" HorizontalAlignment="Left" Margin="156,149,0,0" VerticalAlignment="Top" Click="Ok_Click"/>
    </Grid>
</Window>

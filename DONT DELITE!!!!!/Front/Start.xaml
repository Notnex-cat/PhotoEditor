        <Button Content="OPEN" HorizontalAlignment="Left" Margin="375,217,0,0" VerticalAlignment="Top" Visibility="{Binding ElementVisible, UpdateSourceTrigger=PropertyChanged}" Command="{Binding OpenCommand}"/> <!--Click="Button_Click"-->

